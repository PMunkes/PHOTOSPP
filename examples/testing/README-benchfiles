     second:	no
        exp:	yes
     cutoff:	1.e-6
 max weight:	3.0

MCTESTER:
   particle:	310 (K-STAR)
 mass power:	1
 mass scale:	on
 histograms:	60 bins; [0 , 1.1]

   analysis:	UserTreeAnalysis
     E_test:	0.01/0.49 (10MeV), max 2 photons

